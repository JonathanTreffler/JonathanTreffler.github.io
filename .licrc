[licenses]
# This indicates which are the only licenses that Licensebat will accept.
# The rest will be flagged as not allowed.
accepted = ["LGPL-2.1-only", "LGPL-2.1-or-later", "LGPL-3.0-only", "LGPL-3.0-or-later", "LGPL-2.1", "LGPL-3.0", "LGPL-3.0+", "AGPL-3.0", "AGPL-3.0-only", "AGPL-3.0-or-later", "Apache-2.0", "ECL-2.0", "EFL-2.0", "EUDatagrid", "Expat", "BSD-2-Clause-FreeBSD", "BSD-2-Clause-Views", "BSD-2-Clause", "FTL", "IPA", "iMatix", "Imlib2", "IJG", "Intel-ACPI", "ISC", "MPL-2.0", "MPL-2.0-no-copyleft-exception", "NCSA", "OLDAP-2.7", "CNRI-Python-GPL-Compatible", "Ruby", "SGI-B-2.0", "SMLNJ", "UPL-1.0", "Unlicense", "W3C", "WTFPL", "X11", "XFree86-1.1", "Zlib", "ZPL-2.1", "Artistic-2.0", "ClArtistic", "Sleepycat", "BSL-1.0", "BSD-3-Clause", "CC0", "CECILL-2.0", "BSD-3-Clause-Clear", "ECos-2.0", "MIT", "CC0-1.0", "CC-BY-4.0", "0BSD"]

[dependencies]
ignored=[]

[behavior]
# False by default, if true, it will only run the checks when one of the dependency files or the .licrc file has been modified.
run_only_on_dependency_modification = false
# False by default, if true, it will never block the build.
do_not_block_pr = true
